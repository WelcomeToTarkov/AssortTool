<Window x:Class="AssortEditor.Models.DogtagBarterInputs"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Additional Information" Height="200" Width="300" Background="{DynamicResource WindowBackgroundBrush}">
    <StackPanel Margin="10">
        <TextBlock Text="Enter Level:" />
        <TextBox x:Name="LevelTextBox" Margin="0,5,0,10" />
        
        <TextBlock Text="Enter Side:" />
        <TextBox x:Name="SideTextBox" Margin="0,5,0,10" />
        
        <Button Content="OK" Click="OkButton_Click" HorizontalAlignment="Right" />
    </StackPanel>
</Window>